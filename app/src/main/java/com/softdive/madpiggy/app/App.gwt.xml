<module rename-to="madpiggy">
    
    <define-property name="madpiggy.env" values="yes, no" />
	<property-provider name="madpiggy.env">
	    <![CDATA[
		  	{
			    var ua = window.navigator.userAgent.toLowerCase();
			    if (ua.indexOf('android') != -1 || ua.indexOf('ipad') != -1 || ua.indexOf('ipod') != -1  || ua.indexOf('iphone') != -1 || ua.indexOf('blackberry') != -1) 
			    { 
			    	var url = document.location.href;
			    	if(url.indexOf("file://") === 0 || url.indexOf("local://") === 0)
			    	{
			    		return "yes";
			    	}
			    }
			   
			    return 'no';
		  	}
		]]>
	</property-provider>
    
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.vaadin.components.gwt.polymer.PolymerElements"/>
    <inherits name="com.softdive.madpiggy.app.client.widget.viewpager.ViewPager"/>
    <inherits name="com.googlecode.mgwt.ui.UI"/>
    <inherits name='com.googlecode.gwtphonegap.PhoneGap' />
    <inherits name="com.googlecode.mgwt.mvp.MVP"/>
    <set-property name="mgwt.mainresource.inject" value="no" />
    <inherits name="com.softdive.madpiggy.app.client.widget.celllist.CellList" />
	<inherits name="com.softdive.madpiggy.app.client.util.Util" />
	<inherits name="com.softdive.madpiggy.app.client.storage.Storage" />
	<inherits name="com.softdive.madpiggy.app.client.storage.user.UserStorage" />
	<inherits name="org.fusesource.restygwt.RestyGWT" />
	<inherits name="com.ait.toolkit.gmaps.GMaps" />


    <entry-point class="com.softdive.madpiggy.app.client.AppEntryPoint"/>

    <collapse-all-properties value="true" />
</module>
